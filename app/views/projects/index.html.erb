<p id="notice">
    <%= notice %>
</p>

<h1>Projects</h1>
<!--role="datatable"-->
<table  class="display responsive no-wrap" id="projects" class="display">
    <thead>
        <tr class="success">
            <th>Reference</th>
            <th>Objet du projet</th>
            <th>Nom du Projet</th>
            <th>Description du Projet</th>
            <th>Secteur d'activité</th>
            <th>Point focal CARPA</th>
            <th>Situation du projet</th>
            <th>Partenaire Public</th>
            <th>Partenaire Privé</th>
            <th>Montant de l'investissement</th>
            <th>Procédure</th>
            <th>Durée du contrat</th>
            <th>Date de création du projet</th>
            <th>User</th>
            <!--<th colspan="3"></th>-->
        </tr>
    </thead>

    <tbody>
        <% @projects.each do |project| %>
            <tr>
                <td>
                    <%= link_to project.reference, project %>
                </td>
                <td>
                    <%= project.object_project %>
                </td>
                <td>
                  <%= project.projectName%>
                </td>
                <td>
                  <%= project.projectDescription%>
                </td>
                <td>
                  <%= project.secteurActivite%>
                </td>
                <td>
                    <%= project.focal_point %>
                </td>
                <td>
                    <%= project.project_situation %>
                </td>
                <td>
                    <%= project.public_partenership %>
                </td>
                <td>
                    <%= project.private_partenership %>
                </td>
                <td>
                    <%= project.amount %>
                </td>

               <td>
                 <%= project.procedure %>
               </td>

               <td>
                <%= project.contratDuration %>
               </td>

                <td>
                    <%= project.duration %>
                </td>

                <td>
                    <%= project.user.lName %>
                </td>

                <!--<td>
                    <%= link_to 'Show', project %>
                </td>
                <td>
                    <%= link_to 'Edit', edit_project_path(project) %>
                </td>
                <td>
                    <%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %>
                </td>-->
            </tr>
            <% end %>
    </tbody>
</table>

<br>
<div class="form-actions">

  <%= link_to 'New Project', new_project_path, remote: true,class: 'submit btn btn-success btn-xs round' %>
</div>
