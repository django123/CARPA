<h1>Courrier Sortant</h1>

<table role="datatable">
  <thead>
  <tr class="success">
    <th>Numero d'ordre</th>
    <th>Ref√©rence</th>
    <th>Destination</th>
    <th>User</th>
    <th>Projet</th>
    <th>Date</th>
    <!--<th colspan="3"></th>-->
  </tr>
  </thead>

  <tbody>
  <% @release_couriers.each do |releaseCourier| %>
      <tr>
